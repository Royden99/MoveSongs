<song xmlns="http://openlyrics.info/namespace/2009/song" version="0.8" createdIn="OpenLP 2.4.6" modifiedIn="OpenLP 2.4.6" modifiedDate="2021-01-09T02:53:07">
  <properties>
    <titles>
      <title>Casting Down Vain Imaginations</title>
    </titles>
    <authors>
      <author>Author Unknown</author>
    </authors>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines>Casting down vain imaginations<br/>And all that exalts itself above the Lord,<br/>Bringing into captivity<br/>All thoughts in obedience<br/>To the One Who looks after us.</lines>
    </verse>
  </lyrics>
</song>
