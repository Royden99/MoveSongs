<song xmlns="http://openlyrics.info/namespace/2009/song" version="0.8" createdIn="OpenLP 2.4.6" modifiedIn="OpenLP 2.4.6" modifiedDate="2021-01-09T02:52:48">
  <properties>
    <titles>
      <title>All Hail The Lamb</title>
    </titles>
    <authors>
      <author>Dave Bilborough</author>
    </authors>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines>All hail the Lamb<br/> 	Enthroned on high<br/>Your praise shall be my battle cry<br/>You reign victorious<br/>Forever glorious<br/>Your name is Jesus,<br/>You are the Lamb!</lines>
    </verse>
  </lyrics>
</song>
